{
  "name": "iam-legend",
  "displayName": "IAM Legend",
  "description": "IAM policy actions autocomplete, documentation & wildcard resolution",
  "version": "1.1.45",
  "engines": {
    "vscode": "^1.63.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TastefulElk/iam-legend"
  },
  "galleryBanner": {
    "color": "#232323",
    "theme": "dark"
  },
  "icon": "images/icon.png",
  "keywords": [
    "aws",
    "iam",
    "serverless",
    "cloudformation",
    "sam",
    "yaml",
    "json",
    "sls",
    "terraform"
  ],
  "categories": [
    "Other",
    "Programming Languages",
    "Education"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onLanguage:json",
    "onLanguage:typescript",
    "workspaceContains:**/*.{tf,tfvars}"
  ],
  "main": "./dist/extension.js",
  "publisher": "SebastianBille",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "rimraf dist && webpack",
    "watch": "webpack --watch",
    "package": "rimraf dist && webpack --mode production --devtool hidden-source-map",
    "compile-integration-tests": "tsc -p . --outDir out --types mocha",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "pretest:integration": "yarn run compile-integration-tests && yarn run compile && yarn run lint",
    "test:integration": "node ./out/test/runTest.js",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.1.5",
    "cheerio": "^1.0.0-rc.10",
    "commitlint": "^16.1.0",
    "copy-webpack-plugin": "^10.2.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.7",
    "mocha": "^9.1.4",
    "p-limit": "^4.0.0",
    "puppeteer": "^13.0.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "semantic-release-vsce": "^5.2.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {},
  "jest": {
    "testMatch": [
      "<rootDir>/src/test/unit/**/*.test.ts"
    ]
  }
}
