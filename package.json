{
  "name": "iam-legend",
  "displayName": "IAM Legend",
  "description": "IAM policy actions autocomplete, documentation & wildcard resolution",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TastefulElk/iam-legend"
  },
  "icon": "icon.png",
  "keywords": [
    "aws",
    "iam",
    "serverless",
    "cloudformation",
    "sam",
    "yaml",
    "json",
    "sls"
  ],
  "categories": [
    "Other",
    "Programming Languages",
    "Education"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onLanguage:json"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "rimraf dist && webpack",
    "watch": "webpack --watch",
    "package": "rimraf dist && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "pretest:integration": "yarn run compile-tests && yarn run compile && yarn run lint",
    "test:integration": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "copy-webpack-plugin": "^10.2.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "jest": "^27.4.7",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {},
  "jest": {
    "testMatch": [
      "<rootDir>/src/test/unit/**/*.test.ts"
    ]
  }
}
