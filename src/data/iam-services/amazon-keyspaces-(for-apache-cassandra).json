{
  "serviceName": "Amazon Keyspaces (for Apache Cassandra)",
  "servicePrefix": "cassandra",
  "url": "https://docs.aws.amazon.com/service-authorization/latest/reference/list_amazonkeyspacesforapachecassandra.html",
  "actions": [
    {
      "name": "Alter",
      "description": "Grants permission to alter a keyspace or table",
      "accessLevel": "Write",
      "resourceTypes": [
        "keyspace",
        "table"
      ],
      "conditionKeys": [
        "aws:RequestTag/${TagKey}",
        "aws:TagKeys"
      ],
      "dependentActions": []
    },
    {
      "name": "Create",
      "description": "Grants permission to create a keyspace or table",
      "accessLevel": "Write",
      "resourceTypes": [
        "keyspace",
        "table"
      ],
      "conditionKeys": [
        "aws:RequestTag/${TagKey}",
        "aws:TagKeys"
      ],
      "dependentActions": []
    },
    {
      "name": "Drop",
      "description": "Grants permission to drop a keyspace or table",
      "accessLevel": "Write",
      "resourceTypes": [
        "keyspace",
        "table"
      ],
      "conditionKeys": [],
      "dependentActions": []
    },
    {
      "name": "Modify",
      "description": "Grants permission to INSERT, UPDATE or DELETE data in a table",
      "accessLevel": "Write",
      "resourceTypes": [
        "table*"
      ],
      "conditionKeys": [],
      "dependentActions": []
    },
    {
      "name": "Restore",
      "description": "Grants permission to restore table from a backup",
      "accessLevel": "Write",
      "resourceTypes": [
        "table*"
      ],
      "conditionKeys": [
        "aws:RequestTag/${TagKey}",
        "aws:TagKeys"
      ],
      "dependentActions": []
    },
    {
      "name": "Select",
      "description": "Grants permission to SELECT data from a table",
      "accessLevel": "Read",
      "resourceTypes": [
        "table*"
      ],
      "conditionKeys": [],
      "dependentActions": []
    },
    {
      "name": "TagResource",
      "description": "Grants permission to tag a keyspace or table",
      "accessLevel": "Tagging",
      "resourceTypes": [
        "keyspace",
        "table"
      ],
      "conditionKeys": [
        "aws:RequestTag/${TagKey}",
        "aws:TagKeys"
      ],
      "dependentActions": []
    },
    {
      "name": "UntagResource",
      "description": "Grants permission to untag a keyspace or table",
      "accessLevel": "Tagging",
      "resourceTypes": [
        "keyspace",
        "table"
      ],
      "conditionKeys": [
        "aws:RequestTag/${TagKey}",
        "aws:TagKeys"
      ],
      "dependentActions": []
    }
  ]
